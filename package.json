{
  "name": "cook-club",
  "devDependencies": {
    "nx": "22.3.3"
  },
  "packageManager": "bun@1.3.4",
  "scripts": {
    "kill-ports": "for port in 8787 8788 8789; do lsof -ti :$port | xargs kill -9 2>/dev/null; done; true",
    "dev": "nx run-many -t dev --parallel=10",
    "dev:server": "nx run-many -t dev --exclude=cook-club-app --parallel=10",
    "build": "nx run-many -t build",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "check-types": "nx run-many -t check-types",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tooling/*"
  ],
  "trustedDependencies": [
    "esbuild",
    "nx",
    "sharp",
    "workerd"
  ],
  "version": "1.0.0"
}
